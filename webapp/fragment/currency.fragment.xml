<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<TableSelectDialog contentWidth="35rem" id="currency" title="Para birimleri" confirm="onCurrencySelected" items="{/CurrencySet}">
		<columns>
			<Column width="5rem">
				<header>
					<Text text="Para birimi"/>
				</header>
			</Column>
			<Column width="10rem">
				<header>
					<Text text="Tanımı"/>
				</header>
			</Column>
		</columns>
		<ColumnListItem>
			<cells>
				<Text text="{Advcr}"/>
				<Text text="{Ltext}"/>
				<!--<Text text="{TABLE}"/>-->
			</cells>
		</ColumnListItem>
	</TableSelectDialog>
	<!--search="onF4ValueHelpSearch"-->
</core:FragmentDefinition>